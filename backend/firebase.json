{
  "hosting": {
    "site": "vthacks2021",
    "public": "static",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "run": {
          "serviceId": "backend",
          "region": "northamerica-northeast1"
        }
      }
    ]
  }
}
